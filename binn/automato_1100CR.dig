        <entry>
          <string>Code</string>
          <string>module stimulus (input clk, rst, input [23:0] dado, input [3:0] ref, input intc, rep, output reg ds, output hit, output reg [3:0] comparacao);
  reg [23:0] dadoTrabalho; 
  reg [6:0] cntBits; 
  
  
  reg enHit; 
  always @ (negedge rst or posedge clk) begin
    if (rst==0)begin
      ds &lt;= dado[23];
      dadoTrabalho &lt;= dado; 
      comparacao &lt;= 0;
      enHit &lt;= 1; 
      cntBits &lt;=0; 
    end
    else begin
           cntBits &lt;= cntBits+1;		
           ds &lt;= dadoTrabalho[23];
           dadoTrabalho &lt;= {dadoTrabalho[22:0],1&apos;b0}; 
           comparacao[0] &lt;= dadoTrabalho[23]; 
           
      	   if (hit) 
             if (rep==0)
               enHit &lt;= 0;  
             else if (intc==0) begin
                     comparacao[4:1] &lt;= 4&apos;d0; 
                     cntBits &lt;= 0;  
                   end 
                   else begin
                     comparacao[4:1] &lt;= comparacao[3:0]; 
                   end
            else comparacao[4:1] &lt;= comparacao[3:0];
         

         end
   end

   assign hit = ( (enHit) &amp;&amp; (comparacao == ref) &amp;&amp; (cntBits&gt;=3)? 1 : 0 ); //
endmodule</string>
