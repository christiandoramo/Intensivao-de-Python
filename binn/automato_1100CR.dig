        <entry>
          <string>Code</string>
          <string>module automato(input clk, rst, d, output hit, output reg [1:0] estado);

	always@(posedge clk or negedge rst)
	   begin
		if(rst==0)
		   estado &lt;= 0;
		else
		  case(estado)
			0: estado &lt;= (d==1? 1:0);
			1: estado &lt;= (d==1? 2:0);
			2: estado &lt;= (d==0? 3:2);
			3: estado &lt;= (d==0? 0:1);
		  endcase
	   end
	assign hit = (estado==3 &amp;&amp; d==0? 1:0);
endmodule</string>
