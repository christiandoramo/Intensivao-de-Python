          <string>module conversor7421(input B0,B1,B2,B3, output reg S0,S1,S2,S3);
	always @(*) begin
		if((B3==1 &amp;&amp; B2==0) || (B2==1 &amp;&amp; B1==1 &amp;&amp; B0==1)) begin
			S3 = 1;
		end else if((B2==1 &amp;&amp; B0==0) || (B2==1 &amp;&amp; B1==0 &amp;&amp; B0==1)) begin
			S2 = 1;
		end else if((B3==1 &amp;&amp; B0==1) || (B2==0 &amp;&amp; B1==1) || (B1==1 &amp;&amp; B0==0)) begin
			S1 = 1;
		end else if((B3==0 &amp;&amp; B2==0 &amp;&amp; B0==1) || (B3==0 &amp;&amp; B1==0 &amp;&amp; B0==1) || (B3==1 &amp;&amp; B0==0)) begin
			S0 = 1;
		end
	end
endmodule</string>
